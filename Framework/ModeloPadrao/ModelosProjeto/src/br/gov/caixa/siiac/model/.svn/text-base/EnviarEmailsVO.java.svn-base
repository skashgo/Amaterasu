package br.gov.caixa.siiac.model;

/**
 * @author GIS Consult
 *
 */
public class EnviarEmailsVO {

	private Integer id;
	private String name;
	private Boolean selected;
	
	/**
	 * @param id
	 * @param name
	 * @param selected
	 */
	public EnviarEmailsVO(Integer id, String name, Boolean selected) {
		super();
		this.id = id;
		this.name = name;
		this.selected = selected;
	}

	public Integer getId() {
		return id;
	}
	
	public void setId(Integer id) {
		this.id = id;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public Boolean getSelected() {
		return selected;
	}
	
	public void setSelected(Boolean selected) {
		this.selected = selected;
	}
}
