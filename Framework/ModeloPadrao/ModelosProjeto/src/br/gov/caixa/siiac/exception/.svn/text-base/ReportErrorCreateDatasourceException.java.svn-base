package br.gov.caixa.siiac.exception;

/**
 * Esta classe manipula exceptions genéricas
 * 
 */
public class ReportErrorCreateDatasourceException extends Exception 
{

	/**
	 * Identificador da classe
	 */
	private static final long serialVersionUID = 8238001656871744863L;

	/**
     * Construtor que recebe e envia a mensagem do erro para super classe.
	 * @param mensagem Mensagem recebida
     */
	public ReportErrorCreateDatasourceException(String mensagem) {
        super(mensagem);
    }
    
    
    /**
     * Construtor que recebe e envia a mensagem do erro para super classe.
     * @param mensagem Mensagem recebida
     * @param exc Origem da exception
     */
    public ReportErrorCreateDatasourceException(String mensagem, Throwable exc) {
        super( mensagem, exc );
    }
}
