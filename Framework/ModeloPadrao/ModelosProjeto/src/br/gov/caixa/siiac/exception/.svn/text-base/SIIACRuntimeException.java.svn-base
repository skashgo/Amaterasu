package br.gov.caixa.siiac.exception;

/**
 * @author GIS Consult
 *
 */
public class SIIACRuntimeException extends RuntimeException  
{

	/**
	 * 
	 */
	private static final long serialVersionUID = -176265785435771801L;

	
	/**
	 * SIIACException
	 * @param msg
	 */
	public SIIACRuntimeException(String msg) {
		super(msg);
	}

	/**
	 * SIIACException
	 * @param e
	 * @param msg
	 */
	public SIIACRuntimeException(Throwable e,String msg) {
		super(msg,e);
	}	
}
