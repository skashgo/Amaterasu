package br.gov.caixa.siiac.model.domain;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;

/**
 * @author GIS Consult
 */
@Entity
@Table(name = "iactb015_trilha_historico", schema = "iacsm001")
public class TrilhaHistorico {
	
	public static final Character TRILHA_HISTORICO_ACAO_ID_INCLUSAO = 'I';
	public static final Character TRILHA_HISTORICO_ACAO_ID_EXCLUSAO = 'E';
	public static final Character TRILHA_HISTORICO_ACAO_ID_ALTERACAO = 'A';
	public static final Character TRILHA_HISTORICO_ACAO_ID_NOTIFICACAO_ENVIADA = 'N';
	
	private Integer nuTrilhaHistorico;
	private Integer nuContrato;
	private Integer nuGarantia;
	private Integer nuVerificacaoContrato;
	private Integer nuNotificacao;
	private String coUsuario;
	private String noAcao;
	private Date tsAcao;
	private Character icAcao;
	private String coResponsavelAcao;
	private String nuIpResponsavelAcao;
	private String deCamposAlterados;
	
	public TrilhaHistorico() {
	}
	
	@Id
	@Column(name = "nu_trilha_historico", nullable = false)
	@GeneratedValue(strategy = GenerationType.AUTO, generator = "iacsq018_trilha_historico")
	@SequenceGenerator(name = "iacsq018_trilha_historico", sequenceName = "iacsm001.iacsq018_trilha_historico")
	public Integer getNuTrilhaHistorico() {
		return nuTrilhaHistorico;
	}
	
	public void setNuTrilhaHistorico(Integer nuTrilhaHistorico) {
		this.nuTrilhaHistorico = nuTrilhaHistorico;
	}
	
	@Column(name = "nu_contrato")
	public Integer getNuContrato() {
		return nuContrato;
	}
	
	public void setNuContrato(Integer nuContrato) {
		this.nuContrato = nuContrato;
	}
	
	@Column(name = "nu_garantia")
	public Integer getNuGarantia() {
		return nuGarantia;
	}
	
	public void setNuGarantia(Integer nuGarantia) {
		this.nuGarantia = nuGarantia;
	}
	
	@Column(name = "nu_verificacao_contrato")
	public Integer getNuVerificacaoContrato() {
		return nuVerificacaoContrato;
	}
	
	public void setNuVerificacaoContrato(Integer nuVerificacaoContrato) {
		this.nuVerificacaoContrato = nuVerificacaoContrato;
	}
	
	@Column(name = "nu_notificacao")
	public Integer getNuNotificacao() {
		return nuNotificacao;
	}
	
	public void setNuNotificacao(Integer nuNotificacao) {
		this.nuNotificacao = nuNotificacao;
	}
	
	@Column(name = "co_usuario")
	public String getCoUsuario() {
		return coUsuario;
	}
	
	public void setCoUsuario(String coUsuario) {
		this.coUsuario = coUsuario;
	}
	
	@Column(name = "no_acao")
	public String getNoAcao() {
		return noAcao;
	}
	
	public void setNoAcao(String noAcao) {
		this.noAcao = noAcao;
	}
	
	@Column(name = "ts_acao")
	public Date getTsAcao() {
		return tsAcao;
	}
	
	public void setTsAcao(Date tsAcao) {
		this.tsAcao = tsAcao;
	}
	
	@Column(name = "ic_acao")
	public Character getIcAcao() {
		return icAcao;
	}
	
	public void setIcAcao(Character icAcao) {
		this.icAcao = icAcao;
	}
	
	@Column(name = "co_responsavel_acao", length = 7)
	public String getCoResponsavelAcao() {
		return coResponsavelAcao;
	}
	
	public void setCoResponsavelAcao(String coResponsavelAcao) {
		this.coResponsavelAcao = coResponsavelAcao;
	}
	
	@Column(name = "nu_ip_responsavel_acao")
	public String getNuIpResponsavelAcao() {
		return nuIpResponsavelAcao;
	}
	
	public void setNuIpResponsavelAcao(String nuIpResponsavelAcao) {
		this.nuIpResponsavelAcao = nuIpResponsavelAcao;
	}
	
	@Column (name = "de_campos_alterados", nullable = false)
	public String getDeCamposAlterados() {
		return deCamposAlterados;
	}

	public void setDeCamposAlterados(String deCamposAlterados) {
		this.deCamposAlterados = deCamposAlterados;
	}	
}