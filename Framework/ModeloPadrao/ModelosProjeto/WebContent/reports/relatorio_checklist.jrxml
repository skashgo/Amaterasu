     iactb008_apontamento_checklist."no_aptmnto_checklist" AS nomeApontamento
FROM
     "iacsm001"."iactb005_chklst_srvco_produto" iactb005_chklst_srvco_produto LEFT OUTER JOIN "iacsm001"."iactb010_bloco_chklst_produto" iactb010_bloco_chklst_produto ON iactb005_chklst_srvco_produto."nu_chklst_srvco_vrfco_produto" = iactb010_bloco_chklst_produto."nu_chklst_srvco_vrfco_produto"
