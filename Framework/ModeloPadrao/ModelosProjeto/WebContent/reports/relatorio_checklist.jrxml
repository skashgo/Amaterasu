     LEFT OUTER JOIN "iacsm001"."iactb007_item_verificacao" iactb007_item_verificacao ON iactb011_item_vrfco_cklst_prdto."nu_item_vrfco_checklist" = iactb007_item_verificacao."nu_item_vrfco_checklist"
