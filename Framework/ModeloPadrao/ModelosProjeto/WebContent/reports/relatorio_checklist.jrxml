     LEFT OUTER JOIN "iacsm001"."iactb003_servico_vrfco_produto" iactb003_servico_vrfco_produto ON iactb005_chklst_srvco_produto."nu_servico_verificacao_produto" = iactb003_servico_vrfco_produto."nu_servico_verificacao_produto"
     INNER JOIN "iacsm001"."iactb001_produto" iactb001_produto ON iactb003_servico_vrfco_produto."co_produto" = iactb001_produto."co_produto"
