<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets">
<ui:composition template="/jsp/include/template.xhtml">
	<ui:define name="breadcrumb">
		<h:panelGroup id="pnlBread">
			<h:outputLabel value="#{msgsServicoProduto['BREAD']}" />
		</h:panelGroup>
	</ui:define>
	<ui:define name="conteudo">
		<h:form>
			<a4j:keepAlive beanName="servicoProdutoMB" />
			<rich:panel id="conteudo">
				<br />
				<rich:spacer height="10" />
				<h:graphicImage value="/images/icon_estagiarios_.gif" />
				<h:outputText
					id="lbl_titlePanelCadServicoProdutoCad"
					value="#{msgsServicoProduto['TITLE.panelCadastro']}"
					styleClass="tituloPagina" />
				<br />
				<rich:spacer height="10" />
				<h:panelGroup id="pnlPrincipal" rendered="#{servicoProdutoMB.showPanelProdutoTable}" style="width: 100%;">
					<rich:panel 
						styleClass="panel-externo"
						id="pnlCadastroRestricaoCad">
						<h:panelGrid
							columns="2"
							width="100%"
							rules="all"
							columnClasses="span-6 linhasPro, span-13 linhasPro"
							style="padding:5px; position:static; text-align: left;"
							border="0">
							<h:column>
								<h:outputLabel
									id="lblCodigoProduto"
									styleClass="rotulosPro"
									style="position: static"
									value="#{msgsServicoProduto['LABEL.coProduto']}" />
								<h:outputText
									style="margin-left: 5px;"
									value="#{servicoProdutoMB.coProdutoFormatado}"
									id="txtCodigoProduto"
									tabindex="1"
									size="10" />
							</h:column>
							<h:column>
								<h:outputLabel
									id="lblNomeProduto"
									styleClass="rotulosPro"
									style="position: static"
									value="#{msgsServicoProduto['LABEL.noProduto']}" />
								<h:outputText
									style="margin-left: 5px;"
									value="#{servicoProdutoMB.produto.noProduto}"
									id="txtNomeProduto"
									tabindex="2"
									size="100" />
							</h:column>								
						</h:panelGrid>
						<h:panelGrid 
							columns="4"
							width="100%"
							rules="all"
							columnClasses="span-6 linhasPro, span-6 linhasPro, span-10 linhasPro, span-10 linhasPro"
							style="padding:5px; position:static; text-align: left;"
							border="0">
							<h:column>
								<h:outputLabel
									id="lblOperacaoProduto"
									styleClass="rotulosPro"
									style="position: static"
									value="#{msgsServicoProduto['LABEL.operacao']}" />
								<h:outputText
									style="margin-left: 5px;"
									value="#{servicoProdutoMB.produto.operacaoFormatada}"
									id="txtOperacaoProduto"
									tabindex="3" />
								
							</h:column>
							<h:column>
								<h:outputLabel
									id="lblModalidadeProduto"
									styleClass="rotulosPro"
									style="position: static"
									value="#{msgsServicoProduto['LABEL.modalidade']}" />
								<h:outputText
									style="margin-left: 5px;"
									value="#{servicoProdutoMB.produto.modalidadeFormatada}"
									id="txtModalidadeProduto"
									tabindex="4" />
							</h:column>
							<h:column>	
								<h:outputLabel
									id="lblCategoriaProduto"
									styleClass="rotulosPro"
									style="position: static"
									value="#{msgsServicoProduto['LABEL.categoria']}" />
								<h:outputText
									style="margin-left: 5px;"
									value="#{servicoProdutoMB.txtCategoria}"
									id="txtCategoriaProduto"
									tabindex="5" />
							</h:column>
							
						
						</h:panelGrid>
						<h:panelGrid 
							columns="2"
							width="100%"
							rules="all"
							columnClasses="span-14 linhasPro, span-6 linhasPro"
							style="padding:5px; position:static; text-align: left;"
							border="0">
							<h:column>
								<h:outputLabel
									id="lblSiglaSistemaOrigemProduto"
									styleClass="rotulosPro"
									style="position: static"
									value="#{msgsServicoProduto['LABEL.siglaSistemaOrigem']}" />
								<h:outputText
									style="margin-left: 5px;"
									value="#{servicoProdutoMB.produto.sgSistemaOrigem}"
									id="txtSiglaSistemaOrigemProduto"
									tabindex="7" />
							</h:column>
							<h:column>
								<h:outputLabel
									id="lblTipoPessoaProduto"
									styleClass="rotulosPro"
									style="position: static"
									value="#{msgsServicoProduto['LABEL.tipoPessoa']}" />
								<h:outputText
									style="margin-left: 5px;"
									value="#{servicoProdutoMB.produto.icPfPj}"
									id="txtTipoPessoaProduto"
									tabindex="9" />
							</h:column>
						
						</h:panelGrid>
						<h:panelGrid
							columns="2"
							width="100%"
							rules="all"
							columnClasses="span-14 linhasPro, span-6 linhasPro"
							style="padding:5px; position:static; text-align: left;"
							border="0">
							<h:column rendered="false">
								<h:outputLabel
									id="lblValorLimiteProduto"
									styleClass="rotulosPro"
									style="position: static"
									value="#{msgsServicoProduto['LABEL.valorLimite']}" />
								<h:outputText
									style="margin-left: 5px;"
									value="#{servicoProdutoMB.produto.vrLimiteVerificacao}"
									id="txtValorLimiteProduto"
									tabindex="8" />
							</h:column>
							<h:column>
								<h:outputLabel
									id="lblSituacao"
									styleClass="rotulosPro"
									style="position: static"
									value="#{msgsServicoProduto['LABEL.situacao']}" />
								<h:column rendered="#{servicoProdutoMB.showSituacaoAtivo}">
									<h:graphicImage value="/images/icon_ativo.gif" style="vertical-align: bottom; margin-left: 5px;" />
									<h:outputText value=" - Ativo"/>
								</h:column>
								<h:column rendered="#{servicoProdutoMB.showSituacaoInativo}"> 
									<h:graphicImage value="/images/icon_-delete.gif" style="vertical-align: bottom; margin-left: 5px;" />
									<h:outputText value=" - Inativo" />
								</h:column>
							</h:column>
						</h:panelGrid>
						<div style="text-align:right; width: 100%; margin-right: 10px;">
							<h:commandButton value="#{msgsServicoProduto['ACTION.voltar']}" action="#{servicoProdutoMB.doVoltar}"
											 styleClass="rich-button-cinza rich-button" />
						</div>
							
					</rich:panel>
					
					<br/>
					<div style="float: left; width: 50%;">
						<h:graphicImage value="/images/icon_dados.jpg" />
						<h:outputText 
								id="listaVinc"
								value="#{msgsServicoProduto['TITLE.listaVinc']}"
								styleClass="tituloPagina" />
					</div>
					<div style="float: left; text-align: right; width: 50%;">
						<h:commandButton value="#{msgsServicoProduto['ACTION.incluir']}" action="#{servicoProdutoMB.doIncluir}"
									 styleClass="rich-button" />
				 	</div>
					<br/>

					<rich:dataTable 
						value="#{servicoProdutoMB.listServicoVerificacaoProduto}"
						var="row"
						rows="10"
						width="100%"
						cellspacing="0"
						cellpadding="0"
						border="1"
						rowClasses="tabela-row-1, tabela-row-2"
						onRowMouseOver="this.style.backgroundColor = '#A2B6E5'"
						onRowMouseOut="this.style.backgroundColor = ''">
						<rich:column>
							<f:facet name="header">
								<h:outputText value="#{msgsServicoProduto['TABLE.noServicoVerificacaoProduto']}" />
							</f:facet>
							<h:outputText value="#{row.servicoVerificacao.noServicoVerificacao}" />
						</rich:column>
						<rich:column 
							width="50px"
							headerClass="th-text-center"
							styleClass="iconesCentro">
							<f:facet name="header">
								<h:outputText value="#{msgsServicoProduto['ACTION.acao']}" />
							</f:facet>
							<h:commandLink
								id="lnkUpdate"
								action="#{servicoProdutoMB.doAlterar}"
								style="border-style:none;border-width:0;">
								<h:graphicImage
									id="imgUpdate"
									styleClass="action-icon"
									alt="Alterar"
									title="Alterar"
									value="/images/icon_editar.gif" />
							</h:commandLink>
							<h:commandLink
								id="lnkDelete"
								onclick="if(!confirm('#{msgsCef['MA005']}')) {return false;}"
								action="#{servicoProdutoMB.doExcluir}"
								style="border-style:none;border-width:0;">
								<h:graphicImage
									id="imgDelete"
									styleClass="action-icon"
									alt="Excluir"
									title="Excluir"
									value="/images/icon_-delete.gif" />
							</h:commandLink>
						</rich:column>
						<f:facet name="footer">
							<rich:datascroller
								fastControls="false"
								inactiveStyleClass="classe-datascroller classe-datascroller-unselected"
								selectedStyleClass="classe-datascroller classe-datascroller-selected"
								id="dataFooterProduto"
								renderIfSinglePage="false">
								<f:facet name="first">
									<h:graphicImage value="/images/tabela/seta_primeiro.gif" />
								</f:facet>
								<f:facet name="first_disabled">
									<h:graphicImage value="/images/tabela/seta_primeiro.gif" />
								</f:facet>
								<f:facet name="last">
									<h:graphicImage value="/images/tabela/seta_ultimo.gif" />
								</f:facet>
								<f:facet name="last_disabled">
									<h:graphicImage value="/images/tabela/seta_ultimo.gif" />
								</f:facet>
								<f:facet name="previous">
									<h:graphicImage value="/images/tabela/seta_anterior.gif" />
								</f:facet>
								<f:facet name="previous_disabled">
									<h:graphicImage value="/images/tabela/seta_anterior.gif" />
								</f:facet>
								<f:facet name="next">
									<h:graphicImage value="/images/tabela/seta_proximo.gif" />
								</f:facet>
								<f:facet name="next_disabled">
									<h:graphicImage value="/images/tabela/seta_proximo.gif" />
								</f:facet>
							</rich:datascroller>
						</f:facet>
					</rich:dataTable>
					
				</h:panelGroup>
				<rich:panel styleClass="panel-externo" id="pnlCadastro" rendered="#{servicoProdutoMB.showPanelCadastro}"
							style="padding: 10px;">
					<div class="span-22 clear last">
						<h:outputLabel 
							value="#{msgsServicoProduto['LABEL.produto']}"
							styleClass="rotulosPro"
							style="position: static" />
						<h:outputText
							value="#{servicoProdutoMB.produto.noProduto}"
							id="txtNomeProdutoCad"
							tabindex="1"
							style="margin-left: 5px;"
							size="10" />
					</div>
					<rich:spacer height="10px" styleClass="clear span-1" style="width: 100%;" />
					<br clear="all" />
					<div class="span-17 clear">
						<h:outputLabel
							rendered="#{servicoProdutoMB.showComboServicoVerificacao}" 
							value="#{msgsServicoProduto['LABEL.servicosVerificacao']}"
							styleClass="rotulosPro"
							style="position: static" />
						<h:selectOneMenu
							rendered="#{servicoProdutoMB.showComboServicoVerificacao}"
							value="#{servicoProdutoMB.nuServicoVerificacaoCombo}" 
							styleClass="forms_input"
							style="margin-left: 3px; width: 450px;">
							<f:selectItems value="#{servicoProdutoMB.selectItemServicos}" />
						</h:selectOneMenu>
						
						<h:outputLabel 
							value="#{msgsServicoProduto['LABEL.servicosVerificacao']}"
							styleClass="rotulosPro"
							style="position: static"
							rendered="#{servicoProdutoMB.showLabelServicoVerificacao}" />								
						<h:outputText
							value="#{servicoProdutoMB.servicoVerificacaoProduto.servicoVerificacao.noServicoVerificacao}"
							id="txtNomeServicoVerificacaoCad"
							tabindex="1" size="10"
							style="margin-left: 5px;"
							rendered="#{servicoProdutoMB.showLabelServicoVerificacao}" />
					</div>
					<div class="span-6 last">
						<h:outputLabel 
							value="#{msgsServicoProduto['LABEL.contaAbertaLote']}"
							styleClass="rotulosPro"
							style="position: static" />
						<h:selectOneMenu value="#{servicoProdutoMB.contaAbertaLoteString}" 
							styleClass="forms_input" style="margin-left: 3px;">
							<f:selectItem itemValue="S" itemLabel="#{msgsServicoProduto['LABEL.sim']}" />
							<f:selectItem itemValue="N" itemLabel="#{msgsServicoProduto['LABEL.nao']}" />
						</h:selectOneMenu>
					</div>
					<br clear="all"/>
					<br clear="all"/>
					<div class="span-11 clear">
						<rich:panel style="text-align: center; height: 120px;">
							<f:facet name="header"><h:outputText value="PRAZO (em dias úteis)" /></f:facet>
							<div class="span-3" style="margin: 0 15px;">
								<h:outputLabel 
									value="#{msgsServicoProduto['LABEL.prazoVerificacao']}"
									styleClass="rotulosPro"
									style="position: static;" />
								<rich:inputNumberSpinner 
									id="inputPrazoVerificacaoCad" minValue="1" maxValue="365" 
									disableBrowserAutoComplete="true"
									cycled="false" 
									oninputmouseover="this.title = '#{msgsCef['MH001']}'"
									inputClass="inputNumberSpinner"
									enableManualInput="true" step="1" required="true"
									value="#{servicoProdutoMB.servicoVerificacaoProduto.pzVerificacao}"
									style="margin: 0 auto;">
									<rich:jQuery
										selector="#inputPrazoVerificacaoCad"
										timing="onload"
										query="numeric()" />
								</rich:inputNumberSpinner>
							</div>
							<div class="span-3" style="margin: 0 15px;">
								<h:outputLabel 
									value="#{msgsServicoProduto['LABEL.prazoAvgestao']}"
									styleClass="rotulosPro"
									style="position: static;" />
								<rich:inputNumberSpinner 
									id="inputPrazoAvgestaoCad" minValue="1" maxValue="365" 
									disableBrowserAutoComplete="true"
									cycled="false" 
									oninputmouseover="this.title = '#{msgsCef['MH001']}'"
									inputClass="inputNumberSpinner"
									enableManualInput="true" step="1" required="false"
									value="#{servicoProdutoMB.servicoVerificacaoProduto.pzAvgestao}"
									style="margin: 0 auto;">
									<rich:jQuery
										selector="#inputPrazoAvgestaoCad"
										timing="onload"
										query="numeric()" />
								</rich:inputNumberSpinner>
							</div>
							<div class="span-3" style="margin: 0 15px;">							
								<h:outputLabel 
									value="#{msgsServicoProduto['LABEL.prazoAvsuret']}"
									styleClass="rotulosPro"
									style="position: static;" />
								<rich:inputNumberSpinner 
									id="inputPrazoAvsuretCad" minValue="1" maxValue="365" 
									disableBrowserAutoComplete="true"
									cycled="false" 
									oninputmouseover="this.title = '#{msgsCef['MH001']}'"
									inputClass="inputNumberSpinner"
									enableManualInput="true" step="1" required="false"
									value="#{servicoProdutoMB.servicoVerificacaoProduto.pzAvsuret}"
									style="margin: 0 auto;">
									<rich:jQuery
										selector="#inputPrazoAvsuretCad"
										timing="onload"
										query="numeric()" />
								</rich:inputNumberSpinner>
							</div>
							<br clear="all" />
						</rich:panel>
						<br clear="all" />
					</div>
					<div class="span-11 last prepend-1">
					<rich:panel style="text-align: center; height: 120px;">
						<f:facet name="header"><h:outputText value="PRAZO NOTIFICAÇÃO (em dias úteis)" /></f:facet>
							
							<div class="span-3" style="margin: 0 15px;">
								<h:outputLabel 
									value="#{msgsServicoProduto['LABEL.prazoNotificacaoUnidadeSuperior']}"
									styleClass="rotulosPro"
									style="position: static;" />
								<rich:inputNumberSpinner 
									id="inputPrazoNotificacaoNivel1Cad" minValue="1" maxValue="365" 
									disableBrowserAutoComplete="true"
									cycled="false"
									oninputmouseover="this.title = '#{msgsCef['MH001']}'"
									inputClass="inputNumberSpinner"
									enableManualInput="true" step="1" required="true"
									value="#{servicoProdutoMB.servicoVerificacaoProduto.pzNotificacaoNivel1}"
									style="margin: 0 auto;">
									<rich:jQuery
										selector="#inputPrazoNotificacaoNivel1Cad"
										timing="onload"
										query="numeric()" />
								</rich:inputNumberSpinner>
							</div>
							<div class="span-3" style="margin: 0 15px;">							
								<h:outputLabel 
									value="#{msgsServicoProduto['LABEL.prazoNotificacaoUnidadeMatriz']}"
									styleClass="rotulosPro"
									style="position: static;" />
								<rich:inputNumberSpinner 
									id="inputPrazoNotificacaoNivel2Cad" minValue="1" maxValue="365" 
									disableBrowserAutoComplete="true"
									cycled="false" 
									oninputmouseover="this.title = '#{msgsCef['MH001']}'"
									inputClass="inputNumberSpinner"
									enableManualInput="true" step="1" required="false"
									value="#{servicoProdutoMB.servicoVerificacaoProduto.pzNotificacaoNivel2}"
									style="margin: 0 auto;">
									<rich:jQuery
										selector="#inputPrazoNotificacaoNivel2Cad"
										timing="onload"
										query="numeric()" />
								</rich:inputNumberSpinner>
							</div>
							<div class="span-3" style="margin: 0 15px;">							
								<h:outputLabel 
									value="#{msgsServicoProduto['LABEL.prazoNotificacaoUnidadeResponsavel']}"
									styleClass="rotulosPro"
									style="position: static;" />
								<rich:inputNumberSpinner 
									id="inputPrazoNotificacaoResponsavelCad" minValue="1" maxValue="365" 
									disableBrowserAutoComplete="true"
									cycled="false" 
									oninputmouseover="this.title = '#{msgsCef['MH001']}'"
									inputClass="inputNumberSpinner"
									enableManualInput="true" step="1" required="false"
									value="#{servicoProdutoMB.servicoVerificacaoProduto.pzNotificacaoUnidadeResponsavel}"
									style="margin: 0 auto;">
									<rich:jQuery
										selector="#inputPrazoNotificacaoResponsavelCad"
										timing="onload"
										query="numeric()" />
								</rich:inputNumberSpinner>
							</div>
							<br clear="all" />
						</rich:panel>
						<br clear="all" />
					</div>		
					<br clear="all"/>
					<div>	
						<h:outputLabel value="#{msgsServicoProduto['LABEL.aplicavelA']}"
							styleClass="rotulosPro" />
						<h:selectOneMenu id="cbAcaoServico" value="#{servicoProdutoMB.servicoVerificacaoProduto.icTipoAcaoServico}"
							styleClass="forms_input" style="margin-left: 5px;">
							<f:selectItem itemLabel="Selecione..." itemValue="" />
							<f:selectItems value="#{servicoProdutoMB.selectItemAcaoServico}"/>
						</h:selectOneMenu>
					</div>
					<br clear="all" />
					<br clear="all" />
				</rich:panel>
				<div align="right">
					<br/>
					<h:commandButton id="btnIncluir"
									 value="#{msgsServicoProduto['ACTION.salvar']}" 
									 action="#{servicoProdutoMB.doGravar}"
									 onclick="if(!confirm('#{msgsCef['MA002']}')) {return false;}"
									 rendered="#{servicoProdutoMB.showButtonInserir}"
									 styleClass="rich-button" />
					<h:commandButton id="btnAlterar"
									 value="#{msgsServicoProduto['ACTION.salvar']}" 
									 action="#{servicoProdutoMB.doGravar}"
									 onclick="if(!confirm('#{msgsCef['MA003']}')) {return false;}"
									 rendered="#{servicoProdutoMB.showButtonAlterar}"
									 styleClass="rich-button" />
									 
					<h:commandButton value="#{msgsServicoProduto['ACTION.cancelar']}" 
									 action="#{servicoProdutoMB.doVoltar}"
									 immediate="true"
									 rendered="#{servicoProdutoMB.showButtonVoltarCad}"
									 styleClass="rich-button-cinza rich-button"
									 style="margin-left: 15px;" />
				</div>
			</rich:panel>
		</h:form>
	</ui:define>
</ui:composition>
</html>
