<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets">
<div id="menu">
	<h:form id="formMenuPrincipal">
		<div class="menubg">
			<div class="container_12">
				<ul class="sf-menu">
					<li><h:commandLink
							id="menuItemIndex"
							action="#{applicationMB.toIndex}"
							value="Inicio"
							immediate="true" /></li>
					<h:panelGroup
						rendered="#{applicationMB.showMenuVerificacaoPreventiva}"
						id="pnlGroupMenuVerificacao">
						<li>
							<h:commandLink
								id="menuItemVerificacao"
								action="#{applicationMB.toVerificacaoPreventiva}"
								value="Verificação"
								immediate="true" />
								<ul style="width: 170px;">
									<li style="text-align: left;">
										<h:commandLink
											id="menuItemLogParecer"
											action="#{applicationMB.toLogParecer}"
											value="Parecer"
											rendered="#{applicationMB.showMenuLogParecer}"
											immediate="true" />
									</li>
								</ul>
						</li>
					</h:panelGroup>
					<li><h:commandLink
							id="menuItemRelatorio"
							action="toRelatorio"
							value="Relatórios"
							immediate="true" />
						<ul style="width: 170px;">
							<h:panelGroup
								rendered="#{applicationMB.showMenuRelatorioChecklist}"
								id="pnlGroupMenuRelatorioChecklist">
								<li style="text-align: left;">
									<h:commandLink
										id="menuItemRelatorioChecklist"
										action="#{applicationMB.toRelatorioChecklist}"
										value="Checklist"
										style=""
										immediate="true" />
								</li>
							</h:panelGroup>
							<h:panelGroup
								rendered="#{applicationMB.showMenuRelVerificacao}"
								id="pnlGroupMenuRelVerificacao">
									<li style="text-align: left;"><h:commandLink
										id="menuRelVerificacao"
										action="#{applicationMB.toRelVerificacao}"
										value="Verificação"
										immediate="true" /></li>
							</h:panelGroup>							
						</ul>
					</li>
					<li><h:commandLink
							id="menuCadastro"
							action="toCadastro"
							value="Cadastro"
							immediate="true" />
						<ul style="width: 170px;">
							<h:panelGroup
								rendered="#{applicationMB.showMenuApontamento}"
								id="pnlGroupMenuApontamento">
								<li style="text-align: left;"><h:commandLink
										id="menuItemApontamento"
										action="#{applicationMB.toApontamento}"
										value="Apontamento"
										style=""
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								rendered="#{applicationMB.showMenuBlocoChecklist}"
								id="pnlGroupMenuBloco">
								<li style="text-align: left;"><h:commandLink
										id="menuItemBlocoCheckList"
										action="#{applicationMB.toBlocoChecklist}"
										value="Bloco de Checklist"
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								rendered="#{applicationMB.showMenuCategoriaProduto}"
								id="pnlGroupMenuCategoriaProduto">
								<li style="text-align: left;"><h:commandLink
										id="menuItemCategoriaProduto"
										action="#{applicationMB.toCategoriaproduto}"
										value="Categoria de Produto"
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								rendered="#{applicationMB.showMenuChecklist}"
								id="pnlGroupMenuChecklist">
								<li style="text-align: left;"><h:commandLink
										id="menuItemCheckList"
										action="#{applicationMB.toChecklist}"
										value="Checklist"
										style=""
										rendered="#{applicationMB.showMenuChecklist}"
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								rendered="#{applicationMB.showMenuContrato}"
								id="pnlGroupMenuContrato">
								<li style="text-align: left;"><h:commandLink
										id="menuItemContratoConta"
										action="#{applicationMB.toContrato}"
										value="Contrato/Conta"
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								rendered="#{applicationMB.showMenuGarantia}"
								id="pnlGroupMenuGarantia">
								<li style="text-align: left;"><h:commandLink
										id="menuItemGarantia"
										action="#{applicationMB.toGarantia}"
										value="Garantia"
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								id="pnlGroupMenuInformacao"
								rendered="#{applicationMB.showMenuInformacao}">
								<li style="text-align: left;"><h:commandLink
										id="menuItemInformacao"
										action="#{applicationMB.toInformacao}"
										value="Informação"
										immediate="true" /></li>
							</h:panelGroup>	
							<h:panelGroup
								rendered="#{applicationMB.showMenuItemVerificacao}"
								id="pnlGroupMenuItemVerificacao">
								<li style="text-align: left;"><h:commandLink
										id="menuItemItemVerificacao"
										action="#{applicationMB.toItemVerificacao}"
										value="Item de Verificação"
										style=""
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								id="pnlGroupMenuTemplateNotificacao"
								rendered="#{applicationMB.showMenuTemplateNotificacao}">
								<li style="text-align: left;"><h:commandLink
										id="menuItemTemplateNotificacao"
										action="#{applicationMB.toTemplateNotificacao}"
										value="Modelo Notificação"
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								id="pnlGroupMenuTemplateParecer"
								rendered="#{applicationMB.showMenuTemplateParecer}">
								<li style="text-align: left;"><h:commandLink
										id="menuItemTemplateParecer"
										action="#{applicationMB.toTemplateParecer}"
										value="Modelo Parecer"
										immediate="true" /></li>
							</h:panelGroup>								
							<h:panelGroup
								rendered="#{applicationMB.showMenuPreferencias}"
								id="pnlGroupMenuPreferenciaUsuario">
								<li style="text-align: left;"><h:commandLink
										id="menuItemPreferencia"
										action="#{applicationMB.toPreferenciaUsuario}"
										value="Preferências Usuário"
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								rendered="#{applicationMB.showMenuProduto}"
								id="pnlGroupMenuProduto">
								<li style="text-align: left;"><h:commandLink
										id="menuItemProduto"
										action="#{applicationMB.toProduto}"
										value="Produto"
										style=""
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								rendered="#{applicationMB.showMenuRestricao}"
								id="pnlGroupMenuRestricao">
								<li style="text-align: left;"><h:commandLink
										id="menuItemRestricao"
										action="#{applicationMB.toRestricao}"
										value="Restrição"
										style=""
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								rendered="#{applicationMB.showMenuServicoVerificacao}"
								id="pnlGroupMenuServicoVerificacao">
								<li style="text-align: left;"><h:commandLink
										id="menuItemServicoVerificacao"
										action="#{applicationMB.toServicoVerificacao}"
										value="Serviço de Verificação"
										style=""
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								rendered="#{applicationMB.showMenuTipoGarantia}"
								id="pnlGroupMenuTipoGarantia">
								<li style="text-align: left;"><h:commandLink
										id="menuTipoGarantia"
										action="#{applicationMB.toTipoGarantia}"
										value="Tipo de Garantia"
										style=""
										immediate="true" /></li>
							</h:panelGroup>
							<h:panelGroup
								rendered="#{applicationMB.showMenuUsuario}"
								id="pnlGroupMenuUsuario">
								<li style="text-align: left;"><h:commandLink
										id="menuItemUsuario"
										action="#{applicationMB.toUsuario}"
										value="Usuário"
										immediate="true" /></li>
							</h:panelGroup>			
						</ul></li>
					<li><h:commandLink
							id="menuAjuda"
							action="toAjuda"
							value="Ajuda"
							immediate="true" /></li>
				</ul>
				<br clear="all" />
			</div>
		</div>
		<a4j:status
			id="sts"
			onstart="Richfaces.showModalPanel('panelSituacao');"
			onstop="#{rich:component('panelSituacao')}.hide()">
		</a4j:status>
		<rich:modalPanel
			id="panelSituacao"
			autosized="true">
			<h:panelGrid columns="3">
				<h:graphicImage
					url="/images/ajax-loader.gif"
					style="border:0px" />
				<rich:spacer width="10" />
				<h:outputText
					style="font-size: 15px;"
					value="Processando..." />
			</h:panelGrid>
		</rich:modalPanel>
		<rich:modalPanel
			id="CEFMessagePanel"
			autosized="true"
			width="350"
			styleClass="cef-message-panel"
			onshow="jQuery('.cef-message-button').focus();"
			showWhenRendered="#{facesContext.maximumSeverity != null}">
			<f:facet name="header">
				<h:outputText
					id="output_header_CEFMessagePanel"
					value="#{msgs['APPLICATION.nomeCompleto']}">
				</h:outputText>
			</f:facet>
			<center>
				<rich:messages
					id="messages_CEFMessagePanel"
					showDetail="false"
					showSummary="true"
					errorClass="rotulo"
					warnClass="rotulo"
					infoClass="rotulo"
					fatalClass="rotulo">
					<f:facet name="infoMarker">
						<h:graphicImage
							value="/images/infoicon.gif"
							style="margin-right:10px" />
					</f:facet>
					<f:facet name="errorMarker">
						<h:graphicImage
							value="/images/falta.gif"
							style="margin-right:10px" />
					</f:facet>
					<f:facet name="warnMarker">
						<h:graphicImage
							value="/images/warnIcon2.gif"
							style="margin-right:10px" />
					</f:facet>
				</rich:messages>
				<rich:spacer
					id="spacer_CEFMessagePanel"
					height="20" />
				<h:commandButton
					id="btnCEFMessagePanelHide"
					value="#{msgs['LABEL.LOGIN.ok']}"
					styleClass="rich-button cef-message-button"
					style="width:70px;" />
				<rich:componentControl
					for="CEFMessagePanel"
					attachTo="btnCEFMessagePanelHide"
					operation="hide"
					event="onclick" />
			</center>
		</rich:modalPanel>
	</h:form>
<div style="clear: both"></div>
</div>
</html>