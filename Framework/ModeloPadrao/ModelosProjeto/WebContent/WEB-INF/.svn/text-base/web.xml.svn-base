			<url-pattern>/jsp/agenda/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>2</role-name>
			<role-name>3</role-name>
			<role-name>8</role-name>
			<role-name>9</role-name>
		</auth-constraint>
	</security-constraint>
	<security-constraint>
		<display-name>Categoria Produto</display-name>
		<web-resource-collection>
			<web-resource-name>Categoria Produto</web-resource-name>
