			<role-name>2</role-name>
		</auth-constraint>
	</security-constraint>
	<security-constraint>
		<display-name>Cadastro Garantia</display-name>
		<web-resource-collection>
			<web-resource-name>Cadastro Garantia</web-resource-name>
			<url-pattern>/jsp/garantia/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
			<role-name>1</role-name>
			<role-name>2</role-name>
			<role-name>3</role-name>
