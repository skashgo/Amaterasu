		</auth-constraint>
	</security-constraint>
	<security-constraint>
		<display-name>Cadastro Template Parecer</display-name>
		<web-resource-collection>
			<web-resource-name>Cadastro Template Parecer</web-resource-name>
			<url-pattern>/jsp/template_parecer/*</url-pattern>
		</web-resource-collection>
		<auth-constraint>
