		</auth-constraint>
	</security-constraint>
	<login-config>
		<auth-method>FORM</auth-method>
		<realm-name>SIIACdomain</realm-name>
		<form-login-config>
			<form-login-page>/jsp/login/login.jsf</form-login-page>
			<form-error-page>/erroLogin.jsf</form-error-page>
		</form-login-config>
	</login-config>
	<security-role>
		<description>GESTOR_SISTEMA</description>
		<role-name>1</role-name>
	</security-role>
	<security-role>
		<description>REGIONAL_CONFORMIDADE</description>
		<role-name>2</role-name>
	</security-role>
	<security-role>
		<description>VERIFICADOR_CONFORMIDADE</description>
		<role-name>3</role-name>
	</security-role>
	<security-role>
		<description>GESTOR_PRODUTO</description>
		<role-name>4</role-name>
	</security-role>
	<security-role>
